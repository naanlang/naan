/*
 * nodeLibInit
 *
 *     Initialize NodeJS utilities.
 *
 */

function nodeLibInit(local manifest) {
    manifest = `(nodeLibInit)

    Naan.module.build(module.id, "nodeLib", function(modobj, compobj) {
        compobj.manifest = manifest
    })
}();
